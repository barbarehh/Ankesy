{
    "manifest_version": 3,
    "name": "Ankesy",
    "description": "Phishing Simulation for media literacy training.",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    

    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}